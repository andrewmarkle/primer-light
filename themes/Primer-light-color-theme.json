	"tokenColors": [
		{
			"settings": {
				"foreground": "#333333ff",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "Text",
			"scope": "variable.parameter.function, variable.other",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#969896"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
			"settings": {
				"foreground": "#173591"
			}
		},
		{
			"name": "Delimeters",
			"scope": "punctuation.section.scope, support.function.core",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#A71D5D"
			}
		},
		{
			"name": "Variable, Constant, Blocks, Instance Variables",
			"scope": [
				"variable",
				"variable.other.constant",
				"variable.other.block",
				"variable.other.readwrite.instance",
				"variable.parameter.function"
			],
			"settings": {
				"foreground": "#ED6A43"
			}
		},
		{
			"name": "Functions",
			"scope": "entity.name.function, meta.require, support.function.any-method",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Classes",
			"scope": "support.class, entity.name.class, entity.name.type.class",
			"settings": {
				"foreground": "#0086B3"
			}
		},
		{
			"name": "Classes",
			"scope": "meta.class",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Methods",
			"scope": "keyword.other.special-method",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#A71D5D"
			}
		},
		{
			"name": "Support",
			"scope": "support.function",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Strings, Inherited Class",
			"scope":[
					"string",
					"constant.other.symbol",
					"entity.other.inherited-class",
					"constant.character.escape.scss",
					"constant.character.escape.css",
					"constant.language.symbol"
				],
			"settings": {
				"foreground": "#4180C6"
			}
		},
		{
			"name": "Integers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#0086B3"
			}
		},
		{
			"name": "Tag",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#63a3a4"
			}
		},
		{
			"name": "Attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Attribute Ids",
			"scope": "entity.other.attribute-name.id, punctuation.definition.entity",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Selector",
			"scope": "meta.selector",
			"settings": {
				"foreground": "#A71D5D"
			}
		},
		{
			"name": "Headings",
			"scope": "markup.heading punctuation.definition.heading, entity.name.section",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Units",
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#5C9966"
			}
		},
		{
			"name": "Bold",
			"scope": "markup.bold, punctuation.definition.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Italic",
			"scope": "markup.italic, punctuation.definition.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A71D5D"
			}
		},
		{
			"name": "Code",
			"scope": "markup.raw.inline",
			"settings": {
				"foreground": "#183691"
			}
		},
		{
			"name": "Json",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Link Text",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#ED6A43"
			}
		},
		{
			"name": "Link Url",
			"scope": "meta.link",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Lists",
			"scope": "markup.lists",
			"settings": {
				"foreground": "#ED6A43"
			}
		},
		{
			"name": "Quotes",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Separator",
			"scope": "meta.separator",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A1DF8A"
			}
		},
		{
			"name": "Deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#EB5368"
			}
		},
		{
			"name": "Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#F0C36F"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Escape Characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#795DA3"
			}
		},
		{
			"name": "Interpolation",
			"scope": "punctuation.section.embedded, variable.interpolation",
			"settings": {
				"foreground": "#ab7967"
			}
		},

		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#91B3E0"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
